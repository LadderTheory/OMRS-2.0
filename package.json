{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "cd DemoApp && npm run build",
    "install-client": "cd frontend && npm install",
    "sonar": "node sonar-project.js",
    "test": "jest --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "keycloak-connect": "^11.0.3",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.8.0",
    "supertest": "^6.0.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/archive/"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/frontend/"
    ]
  }
}
